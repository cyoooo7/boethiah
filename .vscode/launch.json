{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron",
      "type": "chrome",
      "request": "attach",
      "port": 8315,
      "cwd": "${workspaceRoot}/app"
    },
    {
      "name": "Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/server/bin/www",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}/server",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "externalConsole": false,
      "sourceMaps": false,
      "outDir": null
    }
  ]
}